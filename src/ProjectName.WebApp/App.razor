<Router AppAssembly="typeof(App).Assembly">
    <Found Context="routeData">
    <RouteView RouteData="routeData" DefaultLayout="typeof(ProjectName.WebApp.Shared.MainLayout)" />
        <FocusOnNavigate RouteData="routeData" Selector="h1" />
    </Found>
    <NotFound>
        <PageTitle>Not found</PageTitle>
    <LayoutView Layout="typeof(ProjectName.WebApp.Shared.MainLayout)">
            <p>Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
